{
	"name": "cms",
	"version": "0.0.0",
	"private": true,
	"main": "package.json",
	"scripts": {
		"dev": "yarn twind:build && sanity dev",
		"build": "yarn twind:build && sanity build",
		"start": "sanity start",
		"lint": "tsc --pretty --noImplicitAny",
		"deploy": "yarn twind:build && sanity deploy",
		"twind": "npx tailwindcss -i ./lib/tailwind.css -o ./static/app.css --watch",
		"twind:build": "npx tailwindcss build -i ./lib/tailwind.css -o ./static/app.css"
	},
	"dependencies": {
		"@sanity/eslint-config-studio": "^2.0.1",
		"@sanity/ui": "^1.0.12",
		"@sanity/vision": "^3.1.4",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "0.5.8",
		"autoprefixer": "^10.4.13",
		"date-fns": "^2.29.3",
		"eslint": "^8.31.0",
		"groq": "^2.33.2",
		"local-constants": "*",
		"local-schema": "*",
		"local-tailwind-base": "*",
		"postcss-load-config": "^4.0.1",
		"postcss": "^8.4.21",
		"prettier": "^2.8.2",
		"react-dom": "^18.2.0",
		"react-icons": "^4.7.1",
		"react": "^18.2.0",
		"sanity": "^3.1.4",
		"styled-components": "^5.3.6",
		"tailwindcss": "^3.2.4",
		"typescript": "^4.9.4",
		"zod": "^3.20.2"
	}
}
